<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="scrollNumber">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textPaddingHorizontal" format="dimension" />
        <attr name="textPaddingVertical" format="dimension" />
        <attr name="subViewPadding" format="dimension"/>
        <attr name="separationString" format="string" />
        <attr name="separationSize" format="dimension" />
        <attr name="separationColor" format="color" />
        <attr name="subTextBackground" format="reference"/>
        <attr name="scrollType">
            <enum name="fromZero" value="0" />
            <enum name="fromCurrent" value="1" />
        </attr>
    </declare-styleable>
</resources>